
  <section class="flex justify-center mt-24">
        <table class="table-auto">
          <thead>
            <tr>
              <th class="px-4 py-2">Title</th>
              <th class="px-4 py-2">Description</th>
              <th class="px-4 py-2">Technology</th>
              
            </tr>
          </thead>
          <% hi.forEach(function(hi) { %>
          <tbody>
            <tr class="bg-blue-100">
              <td class="border px-4 py-2"><%= hi.title %></td>
              <td class="border px-4 py-2"><%= hi.description %></td>
              <td class="border px-4 py-2"><%= hi.technology %></td>
              
            </tr>
          </tbody>
          <% }) %>
        </table>
  </section>